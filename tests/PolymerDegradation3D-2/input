#==========================================================================
# Name:		PolymerDegradation01
# author:	Vinamra Agrawal
# date:		2019 July 18
#
# description:	Simulation polymer degradation due to water diffusion
#		Simulation is perfromed on an octant, with x-y-z symmetries
#		The boundary conditions are:
#			- Dirichlet in X and Y
#			o  mu=1 at x=0, y=0
#			o  mu=1 at x=1, y=1
#			- Periodic in Z
#		The initial condition is a constant with zero water
#		concentration and no damage
#
# usage:	[alamo]$> bin/degradation tests/PolymerDegradation3D/input
#
# output:	tests/PolymerDegradation01/output2
#
# comments:	Elasticity with degraded material doesn't work with AMR
#		Keep amr.max_level = 0
#===========================================================================

#=============== Simulation length =========================================
timestep = 0.01
stop_time = 10.0
#===========================================================================

#=============== AMR parameters ============================================
amr.plot_int = 100
amr.max_level = 0
amr.n_cell = 128 32 16
amr.blocking_factor = 4
amr.regrid_int = 10
amr.grid_eff = 1.0
amr.plot_file = tests/PolymerDegradation3D-2/output
#===========================================================================

#=============== Specify geometry and unrefined mesh =======================
geometry.prob_lo = 0 0 0
geometry.prob_hi = 27.08 6.605 2.05
geometry.is_periodic= 0 0 0
#===========================================================================

#=============== Water diffusion parameters ================================
water.on = true
water.diffusivity = 0.015
water.refinement_threshold = 0.01

water.ic_type = constant
water.ic.value = 0.0
water.bc.lo = REFLECT_EVEN REFLECT_EVEN REFLECT_EVEN
water.bc.hi = EXT_DIR EXT_DIR EXT_DIR
water.bc.lo_1 = 0.0
water.bc.hi_1 = 0.0
water.bc.lo_2 = 0.0
water.bc.hi_2 = 1.0
water.bc.lo_3 = 0.0
water.bc.hi_3 = 1.0
#===========================================================================

#=============== Heat diffusion parameters =================================
thermal.on = false
thermal.diffusivity = 0.01
thermal.refinement_threshold = 0.01
thermal.ic_type = constant
thermal.ic.value = 298.0
thermal.bc.lo = EXT_DIR EXT_DIR EXT_DIR
thermal.bc.hi = EXT_DIR EXT_DIR EXT_DIR
thermal.bc.lo_1 = 368.0
thermal.bc.hi_1 = 368.0
thermal.bc.lo_2 = 368.0
thermal.bc.hi_2 = 368.0
thermal.bc.lo_3 = 368.0
thermal.bc.hi_3 = 368.0
#===========================================================================

#=============== Material model parameters =================================
material.model = isotropic
material.isotropic.lambda = 410.1
material.isotropic.mu = 350.5
#material.isotropic2.E10 = 1.0
#material.isotropic2.E20 = 0.148
#material.isotropic2.Tg = 319.3
#material.isotropic2.Ts = 17.0
#material.isotropic2.temp = 298.0
#===========================================================================

#=============== Damage evolution parameters ===============================
damage.anisotropy = 0
damage.type = water
damage.d_final = 0.85 0.90 
damage.number_of_terms = 2 2
damage.d_i = 0.60 0.40 0.80 0.20
damage.tau_i = 24 96 12 96
damage.t_start_i = 0 12 0 12
#damage.anisotropy = 0
#damage.type = water2
#damage.d_final = 0.85 0.01 0.8
#damage.number_of_terms = 2 2 2
#damage.d_i = 0.70 0.30 0.70 0.30 0.70 0.30
#damage.tau_i = 96 24 96 24 96 24
#damage.t_start_i = 0 0 0 0 0 0
damage.ic_type = constant
damage.ic.value = 0.0 0.0
damage.bc.lo = EXT_DIR EXT_DIR EXT_DIR
damage.bc.hi = EXT_DIR EXT_DIR EXT_DIR
damage.bc.lo_1 = 0.0 0.0
damage.bc.hi_1 = 0.0 0.0 
damage.bc.lo_2 = 0.0 0.0
damage.bc.hi_2 = 0.0 0.0
damage.bc.lo_3 = 0.0 0.0
damage.bc.hi_3 = 0.0 0.0
#===========================================================================

#=============== Elasticity parameters =====================================
elastic.on 				=	true
elastic.bottom_solver 	=	bicgstab

elastic.composite_solve	=	1
elastic.verbose			=	2
elastic.cg_verbose 		=	0
elastic.max_iter		=	1000
elastic.max_fmg_iter 	=	10000
elastic.bottom_max_iter =	100
elastic.max_fixed_iter 	=	500
elastic.linop_maxorder 	=	2
elastic.max_coarsening_level = 100000
elastic.agglomeration 	=	1
elastic.consolidation 	=	1
elastic.tol_rel			=	1.0e-6
elastic.tol_abs			= 	1.0e-6
elastic.cg_tol_rel 		=	1.0e-6
elastic.cg_tol_abs 		=	1.0e-6
elastic.use_fsmooth 	=	1

elastic.body_force		=	0.001 0.0 0.0

# Use the following arguments for a single tension experiment.
#elastic.type            =   single
#elastic.int 			=	100
#elastic.tstart 			=	0.0
#elastic.tend			=	100.0
#elastic.bc.bc_x_min 	=	disp neumann neumann
#elastic.bc.bc_y_min		=	neumann disp neumann
#elastic.bc.bc_z_min 	=	neumann neumann disp
#elastic.bc.bc_x_max 	=	disp trac trac
#elastic.bc.bc_y_max 	=	trac trac trac
#elastic.bc.bc_z_max 	=	trac trac trac
#elastic.bc.x_min = 0.0 0.0 0.0
#elastic.bc.x_max = 0.05 0.0 0.0
#elastic.bc.y_min = 0.0 0.0 0.0
#elastic.bc.y_max = 0.0 0.0 0.0
#elastic.bc.z_min = 0.0 0.0 0.0
#elastic.bc.z_max = 0.0 0.0 0.0

# Use the following arguments for a full tensile test experiment.
elastic.type            = tensile
elasitc.rate            = 1.3   # in mm/min
elastic.test_time       = 0. 3. # this says that I want to do the test at 0 hr and 3 hr of degradation time
elastic.test_duration   = 1.0   # in minutes
elastic.test_dt         = 0.01  # in minutes
#===========================================================================
