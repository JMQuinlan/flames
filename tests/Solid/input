alamo.program = mechanics


plot_file		    = tests/Solid/output

type=static

timestep		    = 0.01
stop_time		    = 1.0

# amr parameters
amr.plot_dt		    = 0.1
amr.max_level		    = 0
amr.n_cell		    = 4 4 4
amr.blocking_factor         = 2

amr.thermo.int = 1
amr.thermo.plot_int = 1

# geometry
geometry.prob_lo	    = 0 0 0
geometry.prob_hi	    = 1 1 1

elastic.nmodels = 1
elastic.model1.E=210 
elastic.model1.nu=0.3

elastic.solver.verbose = 3
elastic.solver.nriters = 1
elastic.solver.max_iter = 30 #30

elastic.bc.type = tension_test
elastic.bc.tension_test.type = uniaxial_stress
elastic.bc.tension_test.disp=(0,1:0,0.01)
